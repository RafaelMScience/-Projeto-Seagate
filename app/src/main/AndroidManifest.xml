<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rafaelm.projetos_beta">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_seagate"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_seagate"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".BarracudaPro"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Barracuda"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Check_Produtos"
            android:colorMode="default"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Ironwolf"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Skyhawk"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Desc_Barracuda"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Desc_Ironwolf"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Desc_Skyhawk"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Contato"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>